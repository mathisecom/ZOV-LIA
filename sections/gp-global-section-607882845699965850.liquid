    <script>
      (function(){
        const el = document.getElementById("valoDeliveryText");
        const today = new Date();

        // ajoute des jours en sautant le dimanche
        function addDaysSkipSunday(date, days){
          const d = new Date(date);
          let added = 0;
          while(added < days){
            d.setDate(d.getDate() + 1);
            if (d.getDay() !== 0) added++; // 0 = dimanche
          }
          return d;
        }

        // expdition le lendemain
        const ship = new Date(today);
        ship.setDate(ship.getDate() + 1);
        if (ship.getDay() === 0) ship.setDate(ship.getDate() + 1); // si dimanche  lundi

        // fentre 4872h ouvres
        const min = addDaysSkipSunday(ship, 2);
        const max = addDaysSkipSunday(ship, 3);

        const opt = { weekday: "short", day: "numeric", month: "short" };
        const s1 = min.toLocaleDateString("fr-FR", opt).replace(".", "");
        const s2 = max.toLocaleDateString("fr-FR", opt).replace(".", "");

        // version courte pour mobile
        el.innerHTML = `En stock  Livraison express <b>${s1}</b>  <b>${s2}</b>`;
      })();
